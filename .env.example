# Example .env for Delegate 1 project
# Notification endpoint for startup script
CALL_MY_PHONE_ENDPOINT=https://your-notify-endpoint.example.com/notify

# Secret/API key for notification (used as password in Basic Auth)
CALL_MY_PHONE_SECRET=your-secret-api-key-here

# Backend and frontend URLs to wait for (optional, used by notify-on-start.js)
BACKEND_URL=http://localhost:8081/public-url
FRONTEND_URL=http://localhost:3000

# Optional: Custom startup notification message
STARTUP_NOTIFY_MESSAGE="Servers started successfully"