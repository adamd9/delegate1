    "@openai/agents": "^0.0.5",
    "@types/imap-simple": "^4.2.10",
    "@types/mailparser": "^3.4.6",
    "@types/node": "^22.0.0",
    "@types/nodemailer": "^7.0.1",
    "@types/ws": "^8.5.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "httpdispatcher": "^2.2.0",
    "https-proxy-agent": "^5.0.1",
    "imap-simple": "^5.1.0",
    "mailparser": "^3.7.4",
    "marked": "^16.1.2",
