{
  "name": "delegate1-server",
  "version": "1.0.0",
  "description": "Delegate1 server with TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "cp src/twiml.xml dist/ && mkdir -p dist/config && cp -R src/config/* dist/config/ || true",
    "start": "node dist/server.js",
    "dev": "nodemon --watch 'src/**/*' --watch '.env' --ext 'ts,js,xml,env' --exec 'ts-node' src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
