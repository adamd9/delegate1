# rename this to .env

OPENAI_API_KEY=

# Twilio Credentials
TWILIO_ACCOUNT_SID=
TWILIO_API_KEY_SID=
TWILIO_API_KEY_SECRET=

# Twilio SMS Credentials
TWILIO_SMS_ACCOUNT_SID=
TWILIO_SMS_AUTH_TOKEN=

# Optional default recipient for outgoing SMS
TWILIO_SMS_DEFAULT_TO=

# Twilio Voice Client  SID
TWILIO_TWIML_APP_SID=

# Public URL without trailing slash
PUBLIC_URL=

# Email Configuration
EMAIL_SMTP_HOST=
EMAIL_SMTP_PORT=
EMAIL_SMTP_USER=
EMAIL_SMTP_PASS=

# Optional default recipient for outgoing email
EMAIL_DEFAULT_TO=
EMAIL_DEFAULT_FROM=

# IMAP Configuration for receiving emails
EMAIL_IMAP_USER=
EMAIL_IMAP_PASSWORD=
EMAIL_IMAP_HOST=
EMAIL_IMAP_PORT=993
EMAIL_IMAP_TLS=true

# Email Safety & Labeling Configuration
EMAIL_PROCESSED_MAILBOX=hk
EMAIL_SENDING_RESTRICTED=true
EMAIL_RECEIVING_FILTER_ENABLED=true

# Session history settings
# Number of previous sessions to replay in the UI (default 3)
SESSION_HISTORY_LIMIT=3
# SQLite database file path (defaults to runtime-data/db/assistant.sqlite)
# DB_FILE=runtime-data/db/assistant.sqlite

# Timezone configuration for agent context (IANA timezone, e.g., "Australia/Sydney", "America/Los_Angeles").
# If not set, the server's local time and timezone will be used.
TIMEZONE=
